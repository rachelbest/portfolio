{
  "name": "rachel-portfolio",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://rachelbest.github.io/portfolio",
  "dependencies": {
    "node-sass": "^7.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "5.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "sass:build": "node-sass src/assets/scss/base.scss src/assets/css/base.css",
    "sass:watch": "npm run sass:build && node-sass -watch scss src/assets/scss/ -o src/assets/css/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.2.3"
  }
}
